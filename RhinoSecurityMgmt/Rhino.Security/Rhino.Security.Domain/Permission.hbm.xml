<?xml version="1.0"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
	<class name="Rhino.Security.Domain.Permission, Rhino.Security.Domain" table="`security_Permissions`" schema="`dbo`">

		<id name="Id" column="`Id`" type="Guid" >
			<generator class="assigned" />
		</id>


		<property name="EntitySecurityKey" column="`EntitySecurityKey`" type="Guid" not-null="false" />

		<property name="Allow" column="`Allow`" type="Boolean" not-null="true" />

		<property name="Level" column="`Level`" type="Int32" not-null="true" />

		<property name="EntityTypeName" column="`EntityTypeName`" type="String" not-null="false" />


		<many-to-one name="EntitiesGroup" column="`EntitiesGroup`" class="Rhino.Security.Domain.EntitiesGroup, Rhino.Security.Domain" />

		<many-to-one name="Operation" column="`Operation`" class="Rhino.Security.Domain.Operation, Rhino.Security.Domain" />

		<many-to-one name="UsersGroup" column="`UsersGroup`" class="Rhino.Security.Domain.UsersGroup, Rhino.Security.Domain" />

		<many-to-one name="User" column="`User`" class="Rhino.Security.Domain.User, Rhino.Security.Domain" />

	</class>
</hibernate-mapping>